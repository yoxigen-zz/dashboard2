{
	"id": "userHealthBars",
	"title": "User's Health",
	"dataSource": "users_health.json",
	"views": [
		{
			"type": "bars",
			"settings": {
				"valueProperty": "health",
				"color": {
					"conditional": {
						"defaultValue": "$primary",
						"conditionGroups": [
							{
								"conditions": [
									{
										"operator": "===",
										"value": 7
									}
								],
								"output": "Purple"
							},
							{
								"conditions": [
									{
										"operator": "<=",
										"value": 3
									}
								],
								"output": "Green"
							}
						]
					}
				}
			}
		}
	]
}