{
	"id": "userHealthBars",
	"title": "User's Health",
	"dataSource": "users_health.json",
	"views": [
		{
			"type": "bars",
			"settings": {
				"valueProperty": "health",
				"color": {
					"conditions": [
						{
							"lessThanOrEquals": 5,
							"value": "$warning"
						},
						{
							"lessThanOrEquals": 3,
							"value": "$error"
						}
					]
				}
			}
		}
	]
}