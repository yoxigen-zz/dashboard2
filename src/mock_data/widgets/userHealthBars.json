{
	"id": "userHealthBars",
	"title": "User's Health",
	"dataSource": "users_health.json",
	"views": [
		{
			"type": "bars",
			"settings": {
				"valueProperty": "health",
				"color": {
					"conditional": [
						{
							"conditions": [
								{
									"operator": "<=",
									"value": 5
								},
								{
									"operator": ">",
									"value": 3
								}
							],
							"value": "$error"
						},
						{
							"conditions": [
								{
									"operator": "<=",
									"value": 3
								}
							],
							"value": "$warning"
						}
					]
				}
			}
		}
	]
}